<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_help"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#000"
    tools:context="com.suyash.dell.medicare.help">


    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="180dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/aboutapp"
        android:isScrollContainer="true"
        android:paddingRight="0dp"
        android:background="#000"
        android:layout_marginTop="25dp"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageButton15"
        android:layout_toStartOf="@+id/imageButton15"
        android:layout_marginRight="11dp"
        android:layout_marginEnd="11dp" />

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="170dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/c1"
        android:isScrollContainer="true"
        android:background="#000"
        android:layout_alignTop="@+id/imageButton13"
        android:layout_alignLeft="@+id/imageButton17"
        android:layout_alignStart="@+id/imageButton17" />

    <TextView
        android:text="CONTACT US"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:textColor="#FFFF"
        android:textStyle="bold"
        android:textSize="15dp"
        android:layout_alignEnd="@+id/imageButton15"
        android:layout_marginEnd="48dp"
        android:id="@+id/textView3" />

    <TextView
        android:text="TALK TO US"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton17"
        android:textColor="#FFFF"
        android:textStyle="bold"
        android:textSize="15dp"
        android:layout_alignStart="@+id/textView3"
        android:id="@+id/textView8" />

    <TextView
        android:text="ABOUT THE APP"
        android:layout_width="wrap_content"
        android:textColor="#FFFF"
        android:textStyle="bold"
        android:textSize="15dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:id="@+id/textView2"
        android:layout_above="@+id/imageButton16"
        android:layout_alignEnd="@+id/imageButton13" />

    <TextView
        android:text="WE CARE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFFF"
        android:textStyle="bold"
        android:textSize="15dp"
        android:id="@+id/textView7"
        android:layout_marginStart="24dp"
        android:layout_below="@+id/imageButton16"
        android:layout_alignStart="@+id/textView2" />

    <ImageButton
        android:id="@+id/imageButton17"
        android:layout_width="170dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/chati"
        android:background="#000"
        android:layout_marginRight="19dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton16" />

    <ImageButton
        android:id="@+id/imageButton16"
        android:layout_width="180dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/count"
        android:background="#000"
        android:layout_marginTop="19dp"
        android:layout_marginRight="19dp"
        android:layout_below="@+id/imageButton13"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/imageButton17" />


</RelativeLayout>
